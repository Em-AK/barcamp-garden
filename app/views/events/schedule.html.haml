- content_for :title do
	= @event.title + " : Schedule"

%h1= yield :title

.socialmedia.label.label-info.pull-right
	.icon-tag
	= @event.slug

- @event.dtstart.to_date.upto(@event.dtend.to_date) do |day|
	= render :partial => 'events/schedule/table', :locals => { :day => day }

= render :partial => 'events/admin_actions'
